{
  "containerDefinitions": [
    {
      "name": "threatzero-api",
      "image": "080014544484.dkr.ecr.us-west-2.amazonaws.com/threatzero-api:094f60fbcc2a92f82ed181c6714cc5463e663d68",
      "cpu": 0,
      "portMappings": [
        {
          "name": "threatzero-api-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "DEPLOYMENT_ENVIRONMENT",
          "value": "production"
        }
      ],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::tz-app-config/threatzero-api-production.env",
          "type": "s3"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "AWS_CLOUDFRONT_DISTRIBUTIONS_APPFILES_PRIVATEKEY",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:080014544484:secret:CloudfrontPrivateKey-BFuuR6"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/threatzero-api/production",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "threatzero-api-production",
  "taskRoleArn": "arn:aws:iam::080014544484:role/task-threatzero-api-production",
  "executionRoleArn": "arn:aws:iam::080014544484:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
